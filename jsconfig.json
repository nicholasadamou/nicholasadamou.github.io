{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$components": ["src/components"],
			"$components/*": ["src/components/*"],
			"$sections": ["src/sections"],
			"$sections/*": ["src/sections/*"],
			"$hooks": ["src/hooks"],
			"$hooks/*": ["src/hooks/*"],
			"$config": ["src/config.js"]
		}
	},
	"include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.svelte"]
}
