{
	"name": "nicholasadamou.com",
	"version": "1.0.0",
	"description": "Nicholas Adamou's Personal Website",
	"author": "Nicholas Adamou <nicholasadamouemail@gmail.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/nicholadamou/nicholasadamou.com"
	},
	"keywords": [
		"svelte"
	],
	"engines": {
		"node": ">=0.12"
	},
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"prepare": "husky install",
		"lint-staged": "lint-staged",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"update": "npx yarn-check -u",
		"clean": "sudo rm -rf .svelte-kit build/ node_modules/ yarn.lock package-lock.json"
	},
	"lint-staged": {
		"*.{js,css,json,md}": [
			"prettier --write"
		],
		"*.js": [
			"eslint --fix"
		]
	},
	"devDependencies": {
		"@beyonk/svelte-google-analytics": "^2.2.0",
		"@sveltejs/adapter-netlify": "1.0.0-next.36",
		"@sveltejs/kit": "next",
		"eslint": "8.5.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "3.2.1",
		"node-sass": "7.0.1",
		"normalize.css": "^8.0.1",
		"prettier": "2.5.1",
		"prettier-plugin-svelte": "2.5.1",
		"svelte": "3.44.3",
		"svelte-preprocess": "4.10.1"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.35",
		"@fortawesome/free-brands-svg-icons": "^5.15.3",
		"@fortawesome/free-regular-svg-icons": "^5.15.3",
		"@fortawesome/free-solid-svg-icons": "^5.15.3",
		"@sveltestack/svelte-query": "1.5.0",
		"husky": "7.0.4",
		"lint-staged": "12.1.4",
		"moment": "^2.29.1",
		"svelte-awesome": "2.4.2",
		"svelte-lazy": "1.0.12",
		"svelte-loading-spinners": "^0.1.7",
		"svelte-scrollto": "^0.2.0",
		"svelte-themer": "0.4.13"
	}
}
