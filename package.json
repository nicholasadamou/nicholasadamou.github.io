{
	"name": "nicholasadamou.com",
	"version": "1.0.0",
	"description": "Nicholas Adamou's Personal Website",
	"author": "Nicholas Adamou <nicholasadamouemail@gmail.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/nicholadamou/nicholasadamou.com"
	},
	"keywords": [
		"svelte"
	],
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "husky install",
		"lint-staged": "lint-staged",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"netlify": "netlify dev",
		"update": "npx yarn-check -u",
		"clean": "sudo rm -rf .svelte-kit build/ node_modules/"
	},
	"lint-staged": {
		"*.{js,css,json,md}": [
			"prettier --write"
		],
		"*.js": [
			"eslint --fix"
		]
	},
	"devDependencies": {
		"@beyonk/svelte-google-analytics": "^2.2.0",
		"@sveltejs/adapter-netlify": "^1.0.0-next.58",
		"@sveltejs/kit": "next",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"node-sass": "7.0.1",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.34.0",
		"svelte-preprocess": "^4.7.4"
	},
	"type": "module",
	"dependencies": {
		"@beyonk/async-script-loader": "^2.2.1",
		"@fortawesome/fontawesome-svg-core": "^1.2.35",
		"@fortawesome/free-brands-svg-icons": "^5.15.3",
		"@fortawesome/free-regular-svg-icons": "^5.15.3",
		"@fortawesome/free-solid-svg-icons": "^5.15.3",
		"@sveltestack/svelte-query": "^1.4.1",
		"broadcast-channel": "^4.9.0",
		"husky": "^7.0.1",
		"lint-staged": "12.1.4",
		"moment": "^2.29.2",
		"netlify": "^10.1.0",
		"netlify-cli": "^3.7.1",
		"netlify-lambda": "^2.0.15",
		"normalize.css": "^8.0.1",
		"svelte-awesome": "^2.3.2",
		"svelte-lazy": "^1.0.10",
		"svelte-loading-spinners": "^0.1.7",
		"svelte-scrollto": "^0.2.0",
		"svelte-themer": "^0.4.11"
	}
}
